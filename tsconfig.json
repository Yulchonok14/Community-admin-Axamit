{
    "compileOnSave": false,
    "compilerOptions": {
        "noImplicitAny": true,
        "baseUrl": "./",
        "outDir": "./dist/out-tsc",
        "sourceMap": false,
        "declaration": false,
        "module": "es6",
        "moduleResolution": "node",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "importHelpers": true,
        "target": "es2016",
        "typeRoots": [
            "node_modules/@types"
        ],
        "lib": [
            "es6",
            "es2018",
            "dom"
        ],
        "types": [
            "jquery"
        ]
    }
}
